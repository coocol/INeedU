package com.eethan.ineedu.model;

import android.R.integer;

import com.eethan.ineedu.databasebeans.Plays;
import com.eethan.ineedu.databasebeans.UserInfo;

public class PlaysModel {

	private Plays plays;

	private UserInfo ownerUserInfo;
	private int lastNum;

	
	public UserInfo getOwnerUserInfo() {
		return ownerUserInfo;
	}

	public void setOwnerUserInfo(UserInfo ownerUserInfo) {
		this.ownerUserInfo = ownerUserInfo;
	}

	public int getLastNum() {
		return lastNum;
	}

	public void setLastNum(int lastNum) {
		this.lastNum = lastNum;
	}

	public Plays getPlays() {
		return plays;
	}

	public void setPlays(Plays plays) {
		this.plays = plays;
	}
}
